  "components": {
    "schemas": {
      "DownloadRequest": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "format": "uri",
            "examples": ["https://..."]
          },
          "path": {
            "type": "string"
          },
          "rename": {
            "type": "string"
          },
          "params": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "examples": ["-N", "4", "-R", "infinite"]
          }
        }
      },
      "DownloadResponse": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "examples": ["https://..."]
          },
          "params": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "examples": ["-N", "4", "-R", "infinite"]
          }
        }
      },
      "DownloadProgress": {
        "type": "object",
        "properties": {
          "process_status": {
            "type": "integer",
            "examples": [0, 1, 2, 3]
          },
          "percentage": {
            "type": "string",
            "examples": ["50%"]
          },
          "speed": {
            "type": "number",
            "format": "float",
            "examples": [7289347]
          },
          "eta": {
            "type": "number",
            "format": "float",
            "examples": [3600]
          }
        },
        "required": ["process_status", "percentage", "speed", "eta"]
      },
      "DownloadInfo": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string"
          },
          "title": {
            "type": "string"
          },
          "thumbnail": {
            "type": "string"
          },
          "resolution": {
            "type": "string"
          },
          "size": {
            "type": "integer"
          },
          "vcodec": {
            "type": "string"
          },
          "acodec": {
            "type": "string"
          },
          "extension": {
            "type": "string"
          },
          "original_url": {
            "type": "string"
          },
          "created_at": {
            "type": "string",
            "format": "date-time"
          }
        },
        "required": [
          "url",
          "title",
          "thumbnail",
          "resolution",
          "size",
          "vcodec",
          "acodec",
          "extension",
          "original_url",
          "created_at"
        ]
      },
      "DownloadOutput": {
        "type": "object",
        "properties": {
          "path": {
            "type": "string"
          },
          "filename": {
            "type": "string"
          },
          "savedFilePath": {
            "type": "string"
          }
        },
        "required": ["path", "filename", "savedFilePath"]
      },
      "ProcessResponse": {
        "type": "object",
        "properties": {
          "progress": {
            "$ref": "#/components/schemas/DownloadProgress"
          },
          "info": {
            "$ref": "#/components/schemas/DownloadInfo"
          },
          "output": {
            "$ref": "#/components/schemas/DownloadOutput"
          },
          "params": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": ["progress", "info", "output", "params"]
      },
      "GetVersionResponse": {
        "type": "object",
        "properties": {
          "rpcVersion": {
            "type": "string"
          },
          "ytdlpVersion": {
            "type": "string"
          }
        },
        "required": ["rpcVersion", "ytdlpVersion"]
      },
      "SetCookiesRequest": {
        "type": "object",
        "properties": {
          "cookies": {
            "type": "string"
          }
        },
        "required": ["cookies"]
      },
      "CustomTemplate": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "minLength": 1
          },
          "name": {
            "type": "string"
          },
          "content": {
            "type": "string",
            "minLength": 1
          }
        },
        "required": ["id", "name", "content"]
      }
    },
    "securitySchemes": {
      "api_key": {
        "type": "apiKey",
        "name": "api_key",
        "in": "header"
      }
    }
